{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "defaultProject": "portfolio",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "portfolio": {
      "root": "apps/portfolio",
      "sourceRoot": "apps/portfolio",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/portfolio",
            "outputPath": "dist/apps/portfolio"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "portfolio:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "portfolio:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "portfolio:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/portfolio"],
          "options": {
            "jestConfig": "apps/portfolio/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/portfolio/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "portfolio-e2e": {
      "root": "apps/portfolio-e2e",
      "sourceRoot": "apps/portfolio-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/portfolio-e2e/cypress.json",
            "tsConfig": "apps/portfolio-e2e/tsconfig.e2e.json",
            "devServerTarget": "portfolio:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "portfolio:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/portfolio-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "md-react-components": {
      "root": "libs/md-react-components",
      "sourceRoot": "libs/md-react-components/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/md-react-components",
            "tsConfig": "libs/md-react-components/tsconfig.lib.json",
            "project": "libs/md-react-components/package.json",
            "entryFile": "libs/md-react-components/src/index.ts",
            "external": ["react", "react-dom", "react-is", "styled-components"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/md-react-components/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/md-react-components/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/md-react-components"],
          "options": {
            "jestConfig": "libs/md-react-components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
